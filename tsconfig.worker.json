{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./public/workers",
    "module": "ES6",
    "moduleResolution": "node",
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["workers/**/*.ts"]
}
